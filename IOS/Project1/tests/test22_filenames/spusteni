./mole _ <stdin >>stdout 2>>stderr; echo $? >>errcode
./mole ' ' <stdin >>stdout 2>>stderr; echo $? >>errcode
./mole # <stdin >>stdout 2>>stderr; echo $? >>errcode
./mole @ <stdin >>stdout 2>>stderr; echo $? >>errcode
./mole '$' <stdin >>stdout 2>>stderr; echo $? >>errcode
./mole '&' <stdin >>stdout 2>>stderr; echo $? >>errcode
./mole '$' <stdin >>stdout 2>>stderr; echo $? >>errcode
./mole '*' <stdin >>stdout 2>>stderr; echo $? >>errcode
./mole '(' <stdin >>stdout 2>>stderr; echo $? >>errcode
